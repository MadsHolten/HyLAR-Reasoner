    skos:prefLabel "nom du fichier d'assistance" .

<#assistanceStop/hasStart> a :AttributeType ;
    :hasAttributeDomain <#assistanceStop> ;
    :hasAttributeRange <#assistanceLaunch> ;
    skos:prefLabel "lien vers l'obsel de départ" .



