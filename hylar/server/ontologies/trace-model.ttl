@prefix : <http://liris.cnrs.fr/silex/2009/ktbs#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# assistance events
<#assistanceLaunch> a :ObselType ;
    :hasSuperObselType <#AssistanceEvent> .

<#assistanceLaunch/sessionBegin> a :AttributeType ;
    :hasAttributeDomain <#assistanceLaunch> ;
    :hasAttributeRange xsd:dateTime ;
    skos:prefLabel "horodatage du début de session utilisateur" .

<#assistanceLaunch/sourceId> a :AttributeType ;
    :hasAttributeDomain <#assistanceLaunch> ;
    :hasAttributeRange xsd:string ;
    skos:prefLabel "identifiant du composant générant l'évènement" .

<#assistanceLaunch/assistanceFile> a :AttributeType ;
    :hasAttributeDomain <#assistanceLaunch> ;
    :hasAttributeRange xsd:string ;
    skos:prefLabel "nom du fichier d'assistance" .



